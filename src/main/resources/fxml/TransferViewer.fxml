<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
         AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

    <BorderPane>
        <center>
            <StackPane>

                <ScrollPane fx:id="transferPane" fitToWidth="true" fitToHeight="true" prefWidth="200" prefHeight="200">
                    <BorderPane>
                        <center>
                            <VBox fx:id="cellPane" />
                        </center>
                    </BorderPane>
                </ScrollPane>

                <Label fx:id="labelInfo"  userData="TRANSFERVIEWER_TRANSFER_NULL" alignment="CENTER" styleClass="text-h2"/>

            </StackPane>
        </center>
    </BorderPane>

</fx:root>

